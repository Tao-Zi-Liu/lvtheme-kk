            "html_content": "<meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>Document</title>\n  <style>\n    /* 父级命名空间，防止样式冲突 */\n    #fw-live-widget .open-button {\n      background-color: rgba(0, 0, 0, 0);\n      display: none;\n      border: none;\n      cursor: pointer;\n      position: fixed;\n      bottom: 140px;\n      right: 15px;\n      z-index: 1000;\n      width: 80px;\n      height: 80px;\n      overflow: hidden;\n      border-radius: 50%;\n      padding: 0;\n    }\n    #fw-live-widget .open-button #fw-floating-button-thumbnail {\n      position: absolute;\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 100%;\n      object-fit: cover;\n      object-position: center;\n      display: block;\n    }\n    #fw-live-widget .icon-center {\n      position: absolute;\n      top: 50%;\n      left: 50%;\n      transform: translate(-50%, -50%);\n      display: flex;\n      align-items: center;\n      justify-content: center;\n    }\n    #fw-live-widget .icon-center svg {\n      width: 40px;\n      height: 40px;\n      display: block;\n    }\n    #fw-live-widget #liveUnActive svg {\n      stroke: currentColor;\n      color: #fff;\n    }\n    #fw-live-widget #liveUnActive svg polygon,\n    #fw-live-widget #liveUnActive svg circle {\n      stroke: currentColor;\n      fill: none;\n    }\n    #fw-live-widget .is-hidden {\n      display: none !important;\n    }\n    #fw-live-widget .open-button #liveActive {\n      background-color: red;\n      color: white;\n      border-radius: 0.625rem;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      gap: 2px;\n      font-size: 1rem;\n      width: 3.125rem;\n      z-index: 1;\n      height: 1.25rem;\n    }\n  </style>\n\n\n  <!-- 初始播放器包裹在容器中 -->\n<div class=\"fw-story-block\">\n  <fw-storyblock channel=\"lavivid_hair\" playlist=\"5zJDLe\" mode=\"pinned\" player_placement=\"bottom-right\" autoplay=\"autoplay\">\n  </fw-storyblock>\n</div>\n\n<div id=\"fw-live-widget\">\n  <button id=\"fw-floating-button\" class=\"open-button\">\n    <img id=\"fw-floating-button-thumbnail\" src=\"\" alt=\"\">\n    <span id=\"liveUnActive\" class=\"icon-center\" style=\"color: #fff;\">\n      <svg width=\"40\" height=\"40\" viewbox=\"0 0 20 20\" xmlns=\"http://www.w3.org/2000/svg\"><polygon fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.1\" points=\"8.5 7 13.5 10 8.5 13\"></polygon><circle fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.1\" cx=\"10\" cy=\"10\" r=\"9\"></circle></svg>\n    </span>\n    <span id=\"liveActive\" class=\"icon-center\">\n      <svg stroke=\"currentColor\" fill=\"currentColor\" stroke-width=\"0\" viewbox=\"0 0 512 512\" height=\"1em\" width=\"1em\" xmlns=\"http://www.w3.org/2000/svg\">\n        <path d=\"M256 8C119 8 8 119 8 256s111 248 248 248 248-111 248-248S393 8 256 8zm115.7 272l-176 101c-15.8 8.8-35.7-2.5-35.7-21V152c0-18.4 19.8-29.8 35.7-21l176 107c16.4 9.2 16.4 32.9 0 42z\">\n        </path>\n      </svg>\n      live\n    </span>\n  </button>\n</div>\n\n\n\n\n<!-- <script async src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js\"></script> -->\n<script async=\"\" type=\"text/javascript\" src=\"https://asset.fwcdn3.com/js/fwn.js\"></script>\n<script>\n\n  const props = {\n    playlist: '5zJDLe',\n    thumbnailUrl: 'https://d23tttlodvxe00.cloudfront.net/img/cms/liveIcon/previewIcon2.png'\n  }\n\n  // 统一事件委托处理\n  document.addEventListener(\"fw:player:quit\", displayBubbleFull);\n\n  // 按钮事件绑定\n  document.getElementById('fw-floating-button').addEventListener('click', openFloatingPlayer);\n\n  // 展示播放器图标\n  function displayBubble() {\n    // 移除已有的 firework 播放器容器（原先使用 jQuery，这里改为原生 JS）\n    document.querySelectorAll(\".fw-story-block\").forEach(function (el) {\n      el.remove();\n    });\n    document.getElementById(\"fw-floating-button\").style.display = \"block\";\n    fetchThumbnail()\n  }\n\n  function displayBubbleFull() {\n    window.localStorage.setItem(\"storyBlock\", \"false\")\n    displayBubble()\n  }\n\n  function hideBubble() {\n    document.getElementById(\"fw-floating-button\").style.display = \"none\";\n  }\n\n  // 展示firework播放器\n  function openFloatingPlayer() {\n    hideBubble();\n    window.localStorage.setItem(\"storyBlock\", \"true\")\n\n    // 使用DOM API创建元素\n    const container = document.createElement('div');\n    container.className = 'fw-story-block';\n\n    const player = document.createElement('fw-storyblock');\n    player.setAttribute('channel', 'lavivid_hair');\n    player.setAttribute('playlist', '5zJDLe');\n    player.setAttribute('mode', 'pinned');\n    player.setAttribute('autoplay', 'true');\n    player.setAttribute('player_placement', 'bottom-right');\n\n    container.appendChild(player);\n    document.body.appendChild(container);\n  }\n\n  // 获取firework视频封面\n  async function fetchThumbnail() {\n    const playlistId = props.playlist\n    try {\n      const response = await fetch(`https://fireworkapi1.com/embed/v2/playlists/${playlistId}/feeds?page_size=1`, {\n        method: 'POST',\n      })\n      const feed = await response.json()\n      const thumbnailUrl = feed?.feed_items?.[0]?.video?.video_posters[0].url ?? null\n      const status = feed?.feed_items?.[0]?.video?.live_stream_status\n      if (thumbnailUrl) {\n        document.getElementById(\"fw-floating-button-thumbnail\").src = thumbnailUrl\n      }\n\n      const isLive = status === 'active'\n      if (isLive) {\n        document.getElementById(\"liveUnActive\").classList.add(\"is-hidden\");\n        document.getElementById(\"liveActive\").classList.remove(\"is-hidden\")\n      } else {\n        document.getElementById(\"liveUnActive\").classList.remove(\"is-hidden\");\n        document.getElementById(\"liveActive\").classList.add(\"is-hidden\")\n      }\n    } catch (error) {\n      document.getElementById(\"fw-floating-button-thumbnail\").src = props.thumbnailUrl\n    }\n  }\n  \n  // true,false\n  window.onload = function () {\n    var storyBlock = window.localStorage.getItem('storyBlock')\n    console.log('storyBlock',storyBlock)\n    if (storyBlock == 'false') {\n      displayBubble();\n      return\n    }\n    if (storyBlock == 'true') {\n      openFloatingPlayer();\n      return\n    }\n  }\n  displayBubble();\n\n</script>"
